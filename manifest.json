{
  "manifest_version": 2,
  "name": "NUX NewTab",
  "version": "0.0.1",
  "minimum_chrome_version": "70.0",
  "description": "New Tab override",
  "author": "Nitro UX",
  "chrome_url_overrides": {
		"newtab": "index.html"
	},
	"incognito": "split",
	"permissions": [
		"activeTab"
	],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
